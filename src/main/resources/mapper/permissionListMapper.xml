<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.theOnlineShop.mapper.permissionListMapper">

    <select id="selectListByRoleName" resultType="com.theOnlineShop.domain.permissionEntity">
        select * from permissionlist where roleName = #{roleName}
    </select>

</mapper>